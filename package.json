{
  "name": "5s-twitter",
  "version": "0.1.0",
  "description": "5S for Twitter",
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=8080 node_modules/.bin/nyc node --harmony-proxies node_modules/.bin/ava",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcruchon/5S-twitter.git"
  },
  "bugs": {
    "url": "https://github.com/gcruchon/5S-twitter/issues"
  },
  "homepage": "https://github.com/Hashnode/mern-starter#readme",
  "author": "Gilles CRUCHON <gilles.cruchon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "cross-env": "1.0.8",
    "cuid": "1.3.8",
    "express": "4.15.3",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "isomorphic-fetch": "2.2.1",
    "limax": "1.5.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sanitize-html": "1.14.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.3.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-dev-server": "2.5.1",
    "webpack-externals-plugin": "1.0.0",
    "webpack-hot-middleware": "2.18.2"
  },
  "devDependencies": {
    "ava": "0.21.0",
    "chai": "4.1.0",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.4",
    "css-modules-require-hook": "4.0.6",
    "cssnano": "3.10.0",
    "enzyme": "2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-plugin-ava": "4.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jsdom": "11.1.0",
    "json-loader": "0.5.4",
    "mock-css-modules": "1.0.0",
    "nock": "9.0.13",
    "nodemon": "1.11.0",
    "null-loader": "0.1.1",
    "nyc": "11.0.3",
    "postcss": "6.0.6",
    "postcss-cssnext": "3.0.2",
    "postcss-focus": "2.0.0",
    "postcss-loader": "2.0.6",
    "postcss-reporter": "4.0.0",
    "pre-commit": "1.2.2",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "15.6.1",
    "redux-ava": "2.2.0",
    "redux-devtools": "3.4.0",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.3.0",
    "rimraf": "2.6.1",
    "sinon": "2.3.8",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": ">=4"
  },
  "ava": {
    "files": [
      "client/**/*.spec.js",
      "server/**/*.spec.js"
    ],
    "source": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "./server/util/setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "client/reducers.js",
      "client/store.js",
      "client/routes.js",
      "server/util/setup-test-env.js",
      "server/config.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
